   ]
  },
  {
   "cell_type": "code",
   "execution_count": 3,
   "id": "d9517cd6",
   "metadata": {},
   "outputs": [],
   "source": [
    "df_cleaned = df.dropna()"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 4,
   "id": "772a575b",
   "metadata": {},
   "outputs": [],
   "source": [
    "df_longer = df_cleaned.melt(\n",
    "    id_vars=\"company_location\",\n",
    "    value_vars=\"job_title\",\n",
    "    var_name=\"jobs\"\n",
    ")"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 5,
   "id": "2ac02943",
   "metadata": {},
   "outputs": [],
   "source": [
    "df_pivot = df_longer.pivot_table(\n",
    "    index=\"company_location\",\n",
    "    columns=\"jobs\",\n",
    "    aggfunc='count'\n",
    ").sort_values(by=('value', 'job_title'), ascending=True)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 6,
   "id": "b4698087",
   "metadata": {},
   "outputs": [],
   "source": [
    "n_items = len(df_pivot.index)\n",
    "y_pos = np.arange(n_items)\n",
    "bar_height = 0.8\n",
    "colors = plt.cm.Dark2.colors"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 7,
   "id": "3d1e4f5e",
   "metadata": {},
   "outputs": [
    {
     "data": {
      "image/png": "",
      "text/plain": [
       "<Figure size 1200x800 with 1 Axes>"
      ]
     },
     "metadata": {},
     "output_type": "display_data"
    }
   ],
   "source": [
    "fig, ax = plt.subplots(figsize=(12, 8))\n",
    "\n",
    "bars = ax.barh(y_pos, df_pivot['value']['job_title'], bar_height, color=colors)\n",
    "\n",
    "ax.bar_label(bars)\n",
    "ax.spines[['right', 'top', 'bottom']].set_visible(False)\n",
    "ax.xaxis.set_visible(False)\n",
    "ax.set_yticks(y_pos)\n",
    "ax.set_yticklabels(df_pivot.index)\n",
    "ax.set_title(\"AI jobs by country\", fontweight=\"bold\")\n",
    "\n",
    "plt.tight_layout()\n",
    "plt.show()"
   ]

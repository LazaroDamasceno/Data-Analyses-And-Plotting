    "import pandas as pd\n",
    "import numpy as np\n",
    "import matplotlib.pyplot as plt"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "id": "6e4e5cf9",
   "metadata": {},
   "outputs": [],
   "source": [
    "df = pd.read_csv(\"ai_job_dataset.csv\")"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 3,
   "id": "2553aa91",
   "metadata": {},
   "outputs": [],
   "source": [
    "df_cleaned = df.dropna()"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 4,
   "id": "6ee93d46",
   "metadata": {},
   "outputs": [],
   "source": [
    "filter = df_cleaned['company_location'] == 'United States'\n",
    "df_filtered = df_cleaned[filter]"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 5,
   "id": "2787d94f",
   "metadata": {},
   "outputs": [],
   "source": [
    "df_melt = df_filtered.melt(\n",
    "    id_vars=\"experience_level\",\n",
    "    value_vars=\"job_title\",\n",
    "    var_name=\"jobs\"\n",
    ")"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 6,
   "id": "e9051a79",
   "metadata": {},
   "outputs": [],
   "source": [
    "df_pivot = df_melt.pivot_table(\n",
    "    index=\"experience_level\",\n",
    "    columns=\"jobs\",\n",
    "    aggfunc=\"count\"\n",
    ").sort_values(by=('value', 'job_title'), ascending=True)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 7,
   "id": "716d7d07",
   "metadata": {},
   "outputs": [],
   "source": [
    "colors = plt.cm.Dark2.colors\n",
    "bar_height = 0.6\n",
    "n_items = len(df_pivot.index)\n",
    "y_pos = np.arange(n_items)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 8,
   "id": "c638c416",
   "metadata": {},
   "outputs": [
    {
     "data": {
      "image/png": "",
      "text/plain": [
       "<Figure size 1200x800 with 1 Axes>"
      ]
     },
     "metadata": {},
     "output_type": "display_data"
    }
   ],
   "source": [
    "fig, ax = plt.subplots(figsize=(12, 8))\n",
    "\n",
    "bars = ax.barh(y_pos, df_pivot['value']['job_title'], bar_height, color=colors)\n",
    "\n",
    "ax.spines[['top', 'right', 'bottom']].set_visible(False)\n",
    "ax.xaxis.set_visible(False)\n",
    "ax.set_yticks(y_pos)\n",
    "ax.set_yticklabels(df_pivot.index)\n",
    "ax.bar_label(bars)\n",
    "ax.set_title(\"AI jobs by experience level in the US\", fontweight=\"bold\")\n",
    "\n",
    "plt.tight_layout()\n",
    "plt.show()"
   ]
